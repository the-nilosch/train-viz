    "   \"metadata\": {},\n",
    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"for i in range(6):\\n\",\n",
    "    \"    for j in range(6):\\n\",\n",
    "    \"        if i == j:\\n\",\n",
    "    \"            continue\\n\",\n",
    "    \"        # skip if already computed in either order\\n\",\n",
    "    \"        if (i, j) in lmc_results or (j, i) in lmc_results:\\n\",\n",
    "    \"            continue\\n\",\n",
    "    \"        print(f\\\"Computing LMC for runs {i} → {j}\\\")\\n\",\n",
